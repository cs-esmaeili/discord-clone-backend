{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/config/*": [
        "./config/*"
      ],
      "@/routes/*": [
        "./routes/*"
      ],
      "@/models/*": [
        "./models/*"
      ],
      "@/utils/*": [
        "./utils/*"
      ]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}